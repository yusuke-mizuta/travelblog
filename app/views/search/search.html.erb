<h1>Search#search</h1>
<p>Find me in app/views/search/search.html.erb</p>
            <%= form_with(url: search_search_path,method: :get,local: true) do |f| %>
                <%= f.text_field :search %>
                <%= f.select :model , options_for_select({ユーザー: :user,ブログ: :blog })%>
                <%= f.submit '検索' %>
                <% end %>

<%= render 'layouts/header', header: @header %>

<div class="container">
	<div class="row">
		<div class="col-xs-8 col-xs-offset-2" style="text-align: center;">
			<h1>検索</h1>
			<%= form_with url: search_search_path, method: :get, local: true do |f| %>
				<%= f.text_field :name %>
  				<%= f.submit "検索", class: "btn-default" %>
			<% end %>
		</div>
	</div>
</div>

<div class="container" style="margin-top: 30px;">
	<div class="row">
		<div class="col-xs-2 col-xs-offset-5">
			<table class="table table-hover table-inverse">
			  <thead>
			    <tr>
			      <th style="text-align: center;"><h3>検索結果</h3></th>
			    </tr>
			  </thead>
			  <tbody>

			  </tbody>
			</table>
		</div>
	</div>
</div>

<%= render 'layouts/footer', footer: @footer %>
